procedure main():
    let a = 7
    let b = 5

    stdout.println ! ( "a + b = %d".format(a + b) )
    stdout.println ! ( "a - b = %d".format(a - b) )
    stdout.println ! ( "a * b = %d".format(a * b) )
    stdout.println ! ( "a / b = %d".format(a / b) )
